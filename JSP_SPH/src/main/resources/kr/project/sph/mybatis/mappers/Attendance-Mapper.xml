<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="Attendance-Mapper">

	<select id="insertAttendanceWork" parameterType="attendance">
		insert into
		attendance(attendanceCode, attendanceWork, mno)
		value=(#{attendanceCode},#{attendanceWork},#{mno})
	</select>
	
	<select id="insertAttendanceLeave" parameterType="attendance">
		insert into
		attendance(attendanceCode, attendanceLeave, mno)
		value=(#{attendanceCode},#{attendanceLeave},#{mno})
	</select>
	
</mapper>
<decorators defaultdir="/WEB-INF/decorator">
	
	<decorator name="Frame" page="decorator.jsp">
		<pattern>*.do</pattern>
	</decorator>
	
	
	<decorator name="iFrame" page="iframe_decorator.jsp">
		<pattern>/**/*.do</pattern>
		<pattern>/main.do</pattern>		
	</decorator> 
			
			
			
	<excludes>
		<pattern>/resources/*</pattern>
		<pattern>login2.jsp</pattern>
	</excludes>	
</decorators>